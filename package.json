{
  "name": "@uma/react",
  "version": "0.1.1",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "txdx test --passWithNoTests --watchAll",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "preview-release": "np --preview --no-publish",
    "prepare-patch": "yarn preview-release patch && yarn version --patch",
    "prepare-minor": "yarn preview-release minor && yarn version --minor",
    "prepare-major": "yarn preview-release major && yarn version --major",
    "release": "np $npm_package_version"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "react": ">=17",
    "typescript": ">=4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  },
  "module": "dist/test.esm.js",
  "size-limit": [
    {
      "path": "dist/test.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@emotion/babel-plugin": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/addons": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/testing-library__jest-dom": "^5.9.5",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "np": "^7.6.0",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "downshift": "^6.1.3"
  }
}
